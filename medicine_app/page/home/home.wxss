.logo{
    height: 200rpx;
    line-height: 200rpx;
    text-align: center;
    color : white;
    background-color:rgb(81,195,50);
    background: url('../../img/logo.png') no-repeat 38% 45%/ 60rpx,rgb(81,
    195,50);
    padding-left: 10%; 
}

.header{
    height: 120rpx;
    background-color: rgb(81,195,50);
    padding: 20rpx 50rpx;
    box-sizing: border-box;
    position: relative;
}

.header icon{
    position: absolute;
    z-index: 2;
    left: 72rpx;
    top:40rpx;
    color : #999999;
}

.search{
    height: 72rpx;
    line-height: 72rpx;
    background-color:white;
    border-radius: 30rpx;
    padding-left: 80rpx;
    font-size: 36rpx;
    padding-top: 4rpx;
}

.placeholder{
    color : #999999;
    font-size: 26rpx;
}

.newsList{
    font-size: 30rpx;
    margin-left: 30rpx;
    line-height: 30rpx;
    padding: 0 0 20rpx;
    border-bottom: 1rpx solid #dfdfdf;
    position: relative;
    height: 180rpx;
    margin-right: 30rpx;
}

.newsList .right {
    width: 230rpx;
    height: 78%;
    position: absolute;
    right: 0;
    top: 11%;
}
.newsList .left {
    width: 63%;
    text-overflow: ellipsis;
    overflow: hidden;
    display: inline-block;
    height: 150rpx;
    word-wrap: break-word; 
    word-break: normal; 
    line-height: 40rpx;
}
.time {
    position: absolute;
    bottom: 30rpx;
    left: 0;
    color : #bebebe;
}

